<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="450"
             x:Class="Clockmaker0.Controls.EditMetaControls.Tabs.CustomBackground">
    <StackPanel>
        <Image x:Name="BackgroundImage" MinHeight="200" MaxHeight="300" Margin="0, 10, 0, 10"/>
        <Button x:Name="ReplaceButton" Content="Replace Background" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center"/>
        <Button x:Name="DeleteButton" Content="Remove Custom Background" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center"/>
    </StackPanel>
</UserControl>
